[Unit]
Description=Gunicorn instance to serve GoodNews
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/GoodNews/backend
ExecStart=/usr/bin/python3.6 -m gunicorn --workers 3 --bind 0.0.0.0:5000 -m 007 backend.wsgi:application

[Install]
WantedBy=multi-user.target
